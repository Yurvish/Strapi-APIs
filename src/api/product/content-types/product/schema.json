{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "brand_name": {
      "type": "string",
      "required": true
    },
    "rating": {
      "type": "decimal",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "garment_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::garment-type.garment-type"
    },
    "neck_design": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::neck-design.neck-design"
    },
    "size": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::size.size"
    },
    "sleeve": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sleeve.sleeve"
    },
    "gender": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gender.gender"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "occasion": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::occasion.occasion"
    },
    "color": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::color.color"
    },
    "original_price": {
      "type": "decimal"
    },
    "rental_price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "title": {
      "type": "string"
    },
    "outfit_wore": {
      "type": "integer"
    },
    "scurity_deposite": {
      "type": "decimal"
    },
    "delivery_date": {
      "type": "date"
    },
    "fabric": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::fabric.fabric"
    },
    "length": {
      "type": "integer"
    },
    "rent_period": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::rent-period.rent-period"
    }
  }
}
